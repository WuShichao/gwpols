Cell[TextData[{
 "This means that we can write\n\t",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"H", "(", "t", ")"}], " ", "=", 
          RowBox[{
           RowBox[{"A", "(", "t", ")"}], " ", 
           RowBox[{"{", 
            RowBox[{
             SubscriptBox["[", 
              RowBox[{"-", "2"}]], 
             RowBox[{
